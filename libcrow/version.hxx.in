#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define LIBCROW_VERSION       $libcrow.version.project_number$ULL
#define LIBCROW_VERSION_STR   "$libcrow.version.project$"
#define LIBCROW_VERSION_ID    "$libcrow.version.project_id$"

#define LIBCROW_VERSION_MAJOR $libcrow.version.major$
#define LIBCROW_VERSION_MINOR $libcrow.version.minor$
#define LIBCROW_VERSION_PATCH $libcrow.version.patch$

#define LIBCROW_PRE_RELEASE   $libcrow.version.pre_release$

#define LIBCROW_SNAPSHOT_SN   $libcrow.version.snapshot_sn$ULL
#define LIBCROW_SNAPSHOT_ID   "$libcrow.version.snapshot_id$"
